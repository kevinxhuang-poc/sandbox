# Copy this file to terraform.tfvars and update the variable values
vpc            = "demo-vpc"
classic_access = false
keys           = ["mykey"]
resource_group = "demo"
region         = "us-east"
tags           = ["owner:joe"]
zones = {
  "zone1" = "us-east-1"
  "zone2" = "us-east-2"
  "zone3" = "us-east-3"
}
subnets = {
  "subnet1" = "10.1.1.0/24"
  "subnet2" = "10.1.2.0/24"
  "subnet3" = "10.1.3.0/24"
}
jh_image              = "ibm-ubuntu-22-04-4-minimal-amd64-1"
jh_profile            = "cx2-2x4"
adm1_image            = "ibm-ubuntu-22-04-4-minimal-amd64-1"
adm1_profile          = "bx2-16x64"
adm1_zone             = "us-east-1"
adm1_subnet           = "subnet1" # subnet[1-3]
adm1_data_volume_size = "1000" # size in GB

